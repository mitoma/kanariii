{
  "manifest_version": 2,
  "name": "KanariIi",
  "description": "kintone customize editor",
  "version": "$VERSION",
  "icons": {
    "128": "kanariii-icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.cybozu.com/k/*",
        "https://*.cybozu-dev.com/k/*",
        "https://*.kintone.com/k/*",
        "https://*.kintone-dev.com/k/*"
      ],
      "js": ["chrome-extension.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["kanariii.js", "media/*"]
}
